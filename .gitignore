# Lots of junk goes in here.
/caches/
/backups/

# Top secret stuff!
/link/.ssh/id_*
/link/.ssh/*.pem
/link/.ssh/known_hosts*

# Don't commit Vim plugins, vim-plug will install them.
/link/.vim/plugged
# still don't commit vim plugins.
/link/.vim/bundle
# this is a symlink
/link/.vim/autoload/plug.vim


# Don't commit rbenv temp files.
/link/.rbenv/shims/
/link/.rbenv/versions/
/link/.rbenv/version

/link/.vim/.netrwhist
# Don't commit tmux plugins, tpm will install them.
/link/.tmux/plugins
# don't commit tmux resurrect files, either
/link/.tmux/resurrect
# or tmux sidebar
/link/.tmux/sidebar

/link/.newsboat/history.*

# Ignore any file with LOCAL in the name (good for per-machine vars/functions)
*LOCAL*

